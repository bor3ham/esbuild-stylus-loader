{
  "name": "esbuild-stylus-loader",
  "version": "0.0.0",
  "description": "esbuild plugin for stylus files",
  "keywords": [
    "esbuild",
    "stylus"
  ],
  "author": {
    "name": "ym-project",
    "email": "ym-project@protonmail.com",
    "url": "https://github.com/ym-project"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ym-project/esbuild-stylus-loader"
  },
  "homepage": "https://github.com/ym-project/esbuild-stylus-loader#readme",
  "bugs": "https://github.com/ym-project/esbuild-stylus-loader/issues",
  "license": "MIT",
  "main": "npm/cjs/index.js",
  "exports": {
    "import": "./npm/esm/index.mjs",
    "require": "./npm/cjs/index.js"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "build:cjs": "tsc --module CommonJS --target ESNext --moduleResolution node --outDir npm/cjs src/*",
    "build:esm": "tsc --module ESNext --target ESNext --moduleResolution node --outDir npm/esm src/*",
    "rename-js-to-mjs": "for file in npm/esm/*.js; do mv $file npm/esm/`basename $file .js`.mjs; done",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run rename-js-to-mjs",
    "clean": "rm -rf npm",
    "lint": "eslint '**/*.{js,ts}'"
  },
  "dependencies": {
    "stylus": "^0.54.8"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/stylus": "^0.48.33",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "ava": "^3.13.0",
    "esbuild": "^0.8.6",
    "eslint": "^7.13.0",
    "husky": "^4.3.0",
    "typescript": "^4.0.5"
  }
}
